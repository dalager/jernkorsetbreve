{
  "name": "jernkorset-website-e2e-tests",
  "version": "1.0.0",
  "description": "E2E tests for Jernkorset website",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
